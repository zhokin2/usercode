      twod0->Sumw2();
      if(twod0->IsA()->InheritsFrom("TH2F")){
	TH2F* Ceff = (TH2F*)twod1->Clone("Ceff");
	Ceff->Divide(twod1,twod0, 1, 1, "B");
	Ceff->Sumw2();
      }
      c1->cd(1);
      TH2F *twod1= (TH2F*)hfile1->Get("h_mapDepth3ADCAmpl225_HE");
      TH2F *twod0= (TH2F*)hfile1->Get("h_mapDepth3_HE");
