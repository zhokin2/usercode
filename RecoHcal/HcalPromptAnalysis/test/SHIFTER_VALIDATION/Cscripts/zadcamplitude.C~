      TH1F* diffADCAmpl_Depth2_HB = new TH1F("diffADCAmpl_Depth2_HB","", 100, 0.,300.);
      int nx = Ceff->GetXaxis()->GetNbins();
      int ny = Ceff->GetYaxis()->GetNbins();
      for (int i=1;i<=nx;i++) {
